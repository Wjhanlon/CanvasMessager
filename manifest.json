{
  "manifest_version": 3,
  "name": "Canvas Messenger",
  "version": "1.0",
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "Icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*.instructure.com/*"
  ],
  "options_page": "options.html"
}